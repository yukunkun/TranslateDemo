        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BaiDuTransActivity" />

        <service android:name=".floatwindowdemo.FloatWindowService" />

        <activity android:name=".floatwindowdemo.MainActivity" />
        <activity android:name=".M3U8Activity"></activity>
    </application>

</manifest>